	background: none;
}

table#calls th.spec {	
	border-left: 1px solid #c1dad7;
	border-top: 0;
	background: #fff url(<%= asset_path 'bullet1.gif' %>) no-repeat;
	font: 10px 'Helvetica Neue', Helvetica, Arial, Verdana, sans-serif;
	padding: 2px 6px 2px 12px;
}

table#calls th.specalt {
	border-left: 1px solid #c1dad7;
	border-top: 0;
	background: #f5fafa url(<%= asset_path 'bullet2.gif' %>) no-repeat;
	font: 10px 'Helvetica Neue', Helvetica, Arial, Verdana, sans-serif;
	color: #b4aa9d;
	padding: 2px 6px 2px 12px;
}

table#calls td.alt {
	background: #F5FAFA;
	color: #B4AA9D;
}

table#calls td {
	border-right: 1px solid #c1dad7;
	border-bottom: 1px solid #c1dad7;
	background: #fff;
	font: 10px 'Helvetica Neue', Helvetica, Arial, Verdana, sans-serif;
	letter-spacing: 1px;
	padding: 2px 12px 2px 12px;
	color: #6d929b;
}

table#call_attrs {
	table-layout:fixed;
	width: 265px;
}

table#call_attrs th {
	font: 11px 'Helvetica Neue', Helvetica, Arial, Verdana, sans-serif;
	color: #6d929b;
	border-right: 1px solid #c1dad7;
	border-bottom: 1px solid #c1dad7;
	border-top: 1px solid #c1dad7;
	letter-spacing: 2px;
	text-transform: uppercase;
	text-align: center;
	padding: 6px 6px 6px 12px;
	background: #cae8ea url(<%= asset_path 'bg_header.jpg' %>) no-repeat;
}

table#call_attrs th.spec {
	border-left: 1px solid #c1dad7;
	border-top: 0;
	background: #fff url(<%= asset_path 'bullet1.gif' %>) no-repeat;
	font: 10px 'Helvetica Neue', Helvetica, Arial, Verdana, sans-serif;
	text-align: left;
}


table#call_attrs td {
	border-right: 1px solid #c1dad7;
	border-bottom: 1px solid #c1dad7;
	background: #fff;
